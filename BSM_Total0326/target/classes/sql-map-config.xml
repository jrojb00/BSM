<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="BoardVO" type="com.mrhi.bsm.model.board.BoardVO" ></typeAlias>
		<typeAlias alias="BoardCategoryVO" type="com.mrhi.bsm.model.board.BoardCategoryVO" ></typeAlias>
		<typeAlias alias="ReplyVO" type="com.mrhi.bsm.model.board.ReplyVO" ></typeAlias>
		<typeAlias alias="UserVO" type="com.mrhi.bsm.model.user.UserVO" ></typeAlias>
		<typeAlias alias="FavoriteCategoryVO" type="com.mrhi.bsm.model.user.FavoriteCategoryVO" ></typeAlias>
		<typeAlias alias="ItemVO" type="com.mrhi.bsm.model.item.ItemVO" ></typeAlias>
		<typeAlias alias="BuyItemVO" type="com.mrhi.bsm.model.item.BuyItemVO" ></typeAlias>
		<typeAlias alias="ItemCategoryVO" type="com.mrhi.bsm.model.item.ItemCategoryVO" ></typeAlias>
		<typeAlias alias="KeywordVO" type="com.mrhi.bsm.model.keyword.KeywordVO" ></typeAlias>
		<typeAlias alias="MappingKeywordVO" type="com.mrhi.bsm.model.keyword.MappingKeywordVO" ></typeAlias>
		<typeAlias alias="TendencyAnalysisVO" type="com.mrhi.bsm.model.item.TendencyAnalysisVO" ></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappings/board-mapping.xml" />	
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/item-mapping.xml" />
		<mapper resource="mappings/keyword-mapping.xml" />	
	</mappers>
</configuration>
